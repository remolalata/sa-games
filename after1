    <script type="text/javascript">
        $(function() {
            $(window).scroll(function() {
                var h = $(window).scrollTop();
                if (h > 100) {
                    $(".back").show(500);
                } else {
                    $(".back").hide(500);
                }
            })
            $(".back a").click(function() {
                $('html,body').animate({
                    scrollTop: 0
                }, 500);
            });
        })
    </script>
